<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Signup_ser</servlet-name>
        <servlet-class>controller.Signup_ser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login_ser</servlet-name>
        <servlet-class>controller.Login_ser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Chat_ser</servlet-name>
        <servlet-class>controller.Chat_ser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddProject_ser</servlet-name>
        <servlet-class>controller.AddProject_ser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Comments</servlet-name>
        <servlet-class>controller.Comments</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Comment_ser</servlet-name>
        <servlet-class>controller.Comment_ser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Submission_ser</servlet-name>
        <servlet-class>controller.Submission_ser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TODO_ser</servlet-name>
        <servlet-class>controller.TODO_ser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signup_ser</servlet-name>
        <url-pattern>/Signup_ser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login_ser</servlet-name>
        <url-pattern>/Login_ser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Chat_ser</servlet-name>
        <url-pattern>/Chat_ser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddProject_ser</servlet-name>
        <url-pattern>/AddProject_ser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Comments</servlet-name>
        <url-pattern>/Comments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Comment_ser</servlet-name>
        <url-pattern>/Comment_ser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Submission_ser</servlet-name>
        <url-pattern>/Submission_ser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TODO_ser</servlet-name>
        <url-pattern>/TODO_ser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Sign.html</welcome-file>
    </welcome-file-list>
</web-app>
